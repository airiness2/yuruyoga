<h4><%= t('view.effect') %><%= t('view.index') %></h4>
<div>
<%= link_to '効果新規作成', new_admin_effect_path %>
</div>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th><%= t('view.effect') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @effects.each do |effect| %>
      <tr>
        <td><%= effect.id %></td>
        <td><%= effect.name %></td>
        <td><%= link_to '詳細', admin_effect_path(effect) %></td>
        <td><%= link_to '編集', edit_admin_effect_path(effect) %></td>
        <td><%= link_to '削除', admin_effect_path(effect), method: :delete, data: { confirm: '本当に削除しますか?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
